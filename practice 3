function productoInterno(a, b) {
    let producto = 0;
    for (let i = 0; i < a.length; i++) {
      producto += a[i] * b[i];
    }
    return producto;
}

function sonOrtogonales(c, b) {
    return productoInterno(a, b) === 0 ? "Los vectores a y b son ortogonales" : "Los vectores a y b no son ortogonales";
}

let vectorA = [3, 2, 1];
let vectorB = [0, 5, -4];

